<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <patterns jcr:primaryType="nt:unstructured">
        <fileupload/>
    </patterns>
    <replacement
        cq:rewriteFinal="{Boolean}true"
        jcr:primaryType="nt:unstructured">
        <fileupload
            jcr:primaryType="nt:unstructured"
            sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
            allowFileNameEditing="\{./allowFileNameEditing}"
            allowUpload="\{./allowUpload}"
            autoUploadDelay="\{./autoUploadDelay}"
            class="cq-droptarget"
            ddGroups="\{./ddGroups}"
            fieldDescription="${./fieldDescription}"
            fieldLabel="${./fieldLabel}"
            fileNameParameter="\{./fileNameParameter}"
            fileReferenceParameter="\{./fileReferenceParameter}"
            name="${./name}"
            required="\{./allowBlank}"
            useHTML5="{Boolean}true"/>
    </replacement>
</jcr:root>
